    "nodemon": "^2.0.6",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.3",
    "react-player": "^2.6.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-window": "^1.8.6",
    "styled-components": "^5.2.1"
